{
    "name": "Heroku: Node.js",
    "keywords": ["node", "express", "heroku"],
    "image": "heroku/nodejs",
    "buildpacks": 
    [{
        "url": "heroku/nodejs"
    }],
    "environments": {
        "test": {
          "formation": {
              "test": {
                "quantity": 1,
                "size": "Standard-1X"
              }
          }
       }
    },
    "addons": [
        {
          "plan": "heroku-postgresql:hobby-dev"
        }
    ],
    "env": {
        "PGSSLMODE": {
            "value": "require"
        }
    }
      
}